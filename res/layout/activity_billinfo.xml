<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/main_bg"
    android:orientation="vertical" >
    
    <TextView
        android:id="@+id/view_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="单据"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/text_bg1" />
    
    <Button
        android:id="@+id/back"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/view_title"
        android:layout_alignBottom="@+id/view_title"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="15dp"
        android:text="返回" />

    <Button
        android:id="@+id/submit"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/view_title"
        android:layout_alignBottom="@+id/view_title"
        android:layout_alignParentRight="true"
        android:layout_marginRight="15dp"
        android:text="刷新" />

    <HorizontalScrollView
        android:id="@+id/container2"
        android:layout_width="fill_parent"
        android:layout_height="215dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/back"
        android:layout_marginTop="10dp"
        android:background="@color/data_bg" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <TableLayout
                android:id="@+id/head"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/label_packagecnt"
                        android:layout_width="50dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:clickable="true"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="箱数"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_s_num"
                        android:layout_width="80dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="发货数量"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_satisfied"
                        android:layout_width="70dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="满足率"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_o_num"
                        android:layout_width="80dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="订货数量"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_s_store"
                        android:layout_width="150dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="发货门店"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_billno"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="单据号"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_s_date"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="发货时间"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_a_num"
                        android:layout_width="80dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="审核数量"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_skucount"
                        android:layout_width="70dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="SKU数"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/label_remark"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="1dip"
                        android:background="@color/title_cell"
                        android:gravity="center_vertical|center_horizontal"
                        android:tag="0"
                        android:text="备注"
                        android:textColor="@color/text_bg1"
                        android:textStyle="bold" />

                </TableRow>
            </TableLayout>

            <LinearLayout
                android:id="@+id/cells"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/head"
                android:layout_marginBottom="1dip"
                android:layout_marginTop="1dip"
                android:orientation="vertical" >
            </LinearLayout>
        </RelativeLayout>
    </HorizontalScrollView>

    <com.baidu.mapapi.map.MapView
        android:id="@+id/bmapsView"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:layout_below="@+id/container2"
        android:clickable="true" >

    </com.baidu.mapapi.map.MapView>

</RelativeLayout>
